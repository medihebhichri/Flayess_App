<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="480.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="utils.controllers.ProjectUpdateController">
    <Label layoutX="14.0" layoutY="14.0" text="Update Project" />
    <Label layoutX="14.0" layoutY="46.0" text="Name" />
    <TextField fx:id="nameField" layoutX="14.0" layoutY="67.0" prefWidth="771.0" />
    <Label layoutX="14.0" layoutY="98.0" text="Description" />
    <TextArea fx:id="descriptionArea" layoutX="14.0" layoutY="119.0" prefHeight="141.0" prefWidth="771.0" />
    <Label layoutX="14.0" layoutY="266.0" text="Start Date" />
    <DatePicker fx:id="startDatePicker" layoutX="14.0" layoutY="288.0" />
    <Label layoutX="192.0" layoutY="266.0" text="End Date" />
    <DatePicker fx:id="endDatePicker" layoutX="192.0" layoutY="288.0" />
    <Label layoutX="369.0" layoutY="266.0" text="Budget" />
    <TextField fx:id="budgetField" layoutX="369.0" layoutY="288.0" prefWidth="150.0" />
    <Label layoutX="14.0" layoutY="332.0" text="Visibility" />
    <ComboBox fx:id="visibilityBox" layoutX="14.0" layoutY="353.0" prefWidth="150.0" />
    <Label layoutX="192.0" layoutY="332.0" text="Tags" />
    <TextField fx:id="tagsField" layoutX="192.0" layoutY="353.0" prefWidth="593.0" />
    <Button fx:id="updateButton" layoutX="670.0" layoutY="428.0" mnemonicParsing="false" onAction="#handleUpdateButton" text="Update" />
    <Button fx:id="cancelButton" layoutX="713.0" layoutY="14.0" mnemonicParsing="false" onAction="#handleCancelButton" text="Cancel" />

</AnchorPane>

